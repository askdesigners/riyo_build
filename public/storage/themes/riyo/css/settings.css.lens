/* assigned to any link that links to the lightbox */
.k-link-lightbox {
	cursor: -webkit-zoom-in;
	cursor: -moz-zoom-in;
}
ul {
	list-style: none;
	padding: 0;
	margin: 0;
}

/* assigned to body by Image Protect plugin if Pinterest pinning is disabled */
body.k-image-protect-nopin a.social-pinterest {
	display: none;
}

/*custom styles from Jan*/

/* Misc */
body {
	padding: 0px;
	margin: 0px;
	font-family: "MuseoSlab-500", serif;
	font-size: 16px;
	font-variant: normal;
	font-weight: 100;
	/*-webkit-font-smoothing: antialiased;*/
	color: #2A2A2A;
}

p {
	font-family: "MuseoSans-500", sans-serif;
}

/* Header + Navigation */
#headnav {
	width: 100%;
	height: 90px;
	background-color: #2A2A2A;
	position: fixed;
	z-index: 999;
	top: 0;
	transition: height 0.3s ease-in-out, opacity 0.2s;
}
#headnav h1 {
	margin-top: 0;
	float: left;
}
#headnav h1 svg {
	max-height: 80px;
	margin-left: 20px;
	margin-top: 8px;
	width: 135px;
}
#headnav nav {
	float: right;
	font-size: 1.1em;
	letter-spacing: 0.05em;
	top: 50%;
	position: relative;
	transform: translateY(-50%);
}
#headnav.stuck {
	height: 40px;
	overflow: hidden;
	opacity: 0.8;
}
nav a {
	color: white;
	text-decoration: none;
	margin-right: 40px;
	font-weight: 300;
}

/* SECTIONS */
section {

	/*	width: 100%;
		height: 100vh;*/
}
.homeSection {
	display: block;
	padding: 4em 0;
	margin: 0;
}
.homeSection h2 {
	font-size: 3em;
	margin: 0;
}
.homeSection p {
	font-size: 1.5em;
	line-height: 1.5em;
	margin: 0;
}
.homeSection .flexy {
	display: flex;
	flex-direction: row;
	flex-wrap: wrap;
	justify-content: space-around;
	align-content: flex-start;
}
.inner_margin {
	max-width: 850px;
	margin: 0 auto;
}
#content {
	margin: 0 auto;
	max-width: 900px;
}
#content h2 {
	font-size: 50px;
}
#content p {
	font-size: 25px;
	font-family: "MuseoSans-500";
}
.showInactiveMembers {
	font-size: 1.3em;
	border: 2.5px #009CFF solid;
	border-radius: 5px;
	padding: 10px;
	color: #009CFF;
	position: relative;
	background-color: transparent;
	display: block;
	margin: 0 auto;
	cursor: pointer;
	transition: background 0.3s, color 0.3s;
}

.showInactiveMembers:hover {
	background-color:#009CFF;
	color: white;
}

.fadeOverlay {
	position: absolute;
	width: 100%;
	height: 80px;
	background: -moz-linear-gradient(top, rgba(255,255,255,0) 0%, rgba(255,255,255,0.84) 44%, rgba(255,255,255,1) 86%, rgba(255,255,255,1) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(255,255,255,0)), color-stop(44%,rgba(255,255,255,0.84)), color-stop(86%,rgba(255,255,255,1)), color-stop(100%,rgba(255,255,255,1))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(255,255,255,0.84) 44%,rgba(255,255,255,1) 86%,rgba(255,255,255,1) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(255,255,255,0.84) 44%,rgba(255,255,255,1) 86%,rgba(255,255,255,1) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(top, rgba(255,255,255,0) 0%,rgba(255,255,255,0.84) 44%,rgba(255,255,255,1) 86%,rgba(255,255,255,1) 100%); /* IE10+ */
	background: linear-gradient(to bottom, rgba(255,255,255,0) 0%,rgba(255,255,255,0.84) 44%,rgba(255,255,255,1) 86%,rgba(255,255,255,1) 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#00ffffff', endColorstr='#ffffff',GradientType=0 ); /* IE6-9 */
}
/* Hero section */
#hero {
	background-color: #CDCDCD;
	height: 100vh;
	max-height: 500px;
	overflow: hidden;
	padding: 0;
}
#home-slideshow-text {
	max-width: 50%;
	max-height: 300px;
	background-color: rgba(42, 42, 42, 0.9);
	position: absolute;
	bottom: 80px;
	color: white;
	padding: 20px;
	padding-bottom: 5px;
	z-index: 1000;
}
#home-slideshow-text a {
	text-decoration: none;
}
#home-slideshow-text h2 {
	color: white;
}
#home-slideshow-text span {
	font-size: 19px;
	margin: 0px;
	line-height: 1.8;
}
#morebutton {
	margin: 0px;
	float: right;
	font-size: 23px;
	border: 2.5px #009CFF solid;
	border-radius: 5px;
	padding: 10px;
	color: #009CFF;
	position: relative;
	transform: translateY(-50%);
}
#morebutton:hover {
	color: white;
	background-color: #009CFF;
}
#home-slideshow-text svg {
	float: left;
	margin-left: 0px;
	padding: 0px;
	margin-bottom: 25px;
}

/* About section */
#onas {
	background-color: white;
}

/* History section */
#historie {
	background-color: #CFCFCF;
}
#historie p{
	font-size: 1.2em;
	column-count: 2;
	-webkit-column-count: 2;
}


/* Members section */
#clenove {
	background-color: white;
	margin: 0 0 50px 0;
}
.members li {
	overflow: hidden;
	width: 120px;
	margin: 10px;
	flex-grow: 1;
	flex-basis: auto;
}
.members li a, .members li span {
	display: block;
}
.members li .img {
	height: 120px;
	width: 120px;
	border-radius: 100px;
	overflow: hidden;
	vertical-align: middle;
	text-align: center;
}
.members li img {
	min-width: 100%;
	min-height: 100%;
	max-width: 150%;
	max-height: 150%;
	height: auto;
	width: auto;
}
.members .memberName {
	text-align: center;
	font-size: 1.1em;
	margin-top: 10px;
	color: #2A2A2A;
	text-decoration: none;
}
.inactiveSection {
	height: 80px;
	overflow: hidden;
	opacity: 0.8;
}

/* Repertoar section */
#repertoar {
	background-color: #009CFF;
	height: 80vh;
	padding-top: 10px;
}
#repertoar h2 {
	color: white;
}

#repertoar .shows li {
	overflow: hidden;
	width: 120px;
	margin: 10px;
	flex-grow: 1;
	flex-basis: auto;
}

/* Sponsors section */
#sponsors {
	background-color: #ECF0F1;
	height: 65vh;
	padding-top: 10px;
}
#sponsors h2 {
	text-align: center;
	font-family: "MuseoSlab-500Italic";
}

/* Reference section */
#reference {
	background-color: white;
	height: 80vh;
}

/* TABLET
----------------------------------------------- */
@media only screen and (min-width:768px) and (max-width:959px) {
	#container {
		width: 728px;
	}
	ol.thumbs li {
		width: 207px;
	}
	div.mcol {
		width: 490px;
	}
	div.rcol {
		font-size: 90%;
	}
	.kevent .article_body {
		padding-right: 0;
	}

	/*body::before{content: "TABLET";font-weight: bold;display: block;text-align: center;background: rgba(255,255,0, 0.9);position: fixed;top: 0;left: 0;right: 0;z-index: 99;}*/
}

/* MOBILE (PORTRAIT/320PX)
----------------------------------------------- */
@media only screen and (max-width:767px) {
	#container {
		width: 280px;
	}
	header h1 {
		display: block;
	}
	header.top {
		margin-top: 2rem;
	}
	ol.thumbs li {
		width: 280px;
	}
	div.mcol {
		width: 280px;
		float: none;
		margin: 0 0 1.1667rem 0;
	}
	div.rcol {
		width: 100%;
		margin: 0 0 24px 0;
		font-size: 90%;
	}
	body.k-source-content div.head {
		width: 280px;
	}
	body.k-source-content div.head h2 {
		width: 280px;
	}
	.kevent .article_body {
		padding-right: 0;
	}
	header.content .share {
		display: none;
	}

	/* not enough room */
	header.content span.entry-type {
		display: none;
	}
	.social {
		float: none;
		margin: 0 0 24px 0;
	}

	/*body::before{content: "MOBILE PORTRAIT";font-weight: bold;display: block;text-align: center;background: rgba(255,255,0, 0.9);position: fixed;top: 0;left: 0;right: 0;z-index: 99;}*/
}

/* MOBILE (LANDSCAPE/480PX)
----------------------------------------------- */
@media only screen and (min-width:480px) and (max-width:767px) {
	body.k-source-content div.head {
		width: 436px;
	}
	div#twocol div.rcol ol.actions li {
		float: left;
		margin: 0 24px 0 0;
	}
	header.content .share {
		display: block;
	}
	header.content div.kshare {
		right: 20px;
	}

	/*body::before{content: "MOBILE LANDSCAPE";font-weight: bold;display: block;text-align: center;background: rgba(255,255,0, 0.9);position: fixed;top: 0;left: 0;right: 0;z-index: 99;}*/
}

